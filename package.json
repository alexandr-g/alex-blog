{
  "name": "alex-blog",
  "description": "Alexander Grischuk personal blog based on GatsbyJS starter kit with the Casper theme.",
  "version": "0.0.2",
  "author": "Alexander Grischuk <alexgrischuk@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "yarn clean && yarn build:pp && gh-pages -d public",
    "build:prod": "yarn clean && yarn build && yarn serve",
    "clean": "rm -rf .cache && rm -rf public",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public --ignore-pattern static .",
    "format": "prettier '**/*.{js,jsx,css}' --write",
    "new:post": "plop"
  },
  "dependencies": {
    "babel-eslint": "10.1.0",
    "babel-plugin-lodash": "3.3.4",
    "classnames": "2.2.6",
    "gatsby": "1.9.279",
    "gatsby-link": "1.6.46",
    "gatsby-pagination": "1.2.0",
    "gatsby-plugin-catch-links": "1.0.26",
    "gatsby-plugin-feed": "1.3.25",
    "gatsby-plugin-google-analytics": "1.0.31",
    "gatsby-plugin-manifest": "1.0.27",
    "gatsby-plugin-nprogress": "1.0.14",
    "gatsby-plugin-offline": "1.0.18",
    "gatsby-plugin-react-helmet": "1.0.8",
    "gatsby-plugin-sharp": "2.6.30",
    "gatsby-plugin-sitemap": "1.2.25",
    "gatsby-plugin-twitter": "1.0.20",
    "gatsby-remark-autolink-headers": "1.4.19",
    "gatsby-remark-copy-linked-files": "1.5.37",
    "gatsby-remark-images": "1.5.67",
    "gatsby-remark-prismjs": "1.2.24",
    "gatsby-remark-responsive-iframe": "1.4.20",
    "gatsby-source-filesystem": "1.5.39",
    "gatsby-transformer-json": "1.0.20",
    "gatsby-transformer-remark": "1.7.44",
    "lodash": "4.17.20",
    "lodash-webpack-plugin": "0.11.5",
    "moment": "2.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-gravatar": "2.6.3",
    "react-helmet": "6.1.0",
    "react-scroll": "1.8.1",
    "react-share": "1.19.1",
    "react-social-icons": "2.9.0"
  },
  "devDependencies": {
    "eslint": "7.8.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "gh-pages": "3.1.0",
    "plop": "2.7.4",
    "prettier": "2.1.1"
  }
}
