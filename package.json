{
  "name": "alex-blog",
  "description": "Alexander Grischuk personal blog based on GatsbyJS starter kit with the Casper theme.",
  "version": "0.0.2",
  "author": "Alexander Grischuk <alexgrischuk@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "10.x.x || 12.x.x"
  },
  "scripts": {
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "yarn clean && yarn build:pp && gh-pages -d public",
    "build:prod": "yarn clean && yarn build && yarn serve",
    "clean": "rm -rf .cache && rm -rf public",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public --ignore-pattern static .",
    "format": "prettier '**/*.{js,jsx,css}' --write"
  },
  "dependencies": {
    "babel-eslint": "^10.0.0",
    "classnames": "^2.2.6",
    "gatsby": "^2.20.18",
    "gatsby-link": "^2.0.0",
    "gatsby-pagination": "^1.1.0",
    "gatsby-plugin-catch-links": "^2.0.0",
    "gatsby-plugin-feed": "^2.0.0",
    "gatsby-plugin-google-analytics": "^2.0.0",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-nprogress": "^2.0.0",
    "gatsby-plugin-offline": "^3.0.0",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-sitemap": "^2.0.0",
    "gatsby-plugin-twitter": "^2.0.0",
    "gatsby-remark-autolink-headers": "^2.0.0",
    "gatsby-remark-copy-linked-files": "^2.0.0",
    "gatsby-remark-images": "^3.0.0",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.0",
    "gatsby-source-filesystem": "^2.0.0",
    "gatsby-transformer-json": "^2.0.0",
    "gatsby-transformer-remark": "^2.7.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-gravatar": "^2.6.3",
    "react-helmet": "^6.0.0",
    "react-scroll": "^1.7.16",
    "react-share": "^1.15.1",
    "react-social-icons": "^2.7.0",
    "react-twitter-widgets": "^1.4.0"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "gh-pages": "2.2.0",
    "plop": "2.6.0",
    "prettier": "2.0.4",
    "remark-cli": "8.0.0"
  }
}
