{
  "name": "alex-blog",
  "description": "Alexander Grischuk personal blog based on GatsbyJS starter kit with the Casper theme.",
  "version": "0.0.2",
  "author": "Alexander Grischuk <alexgrischuk@gmail.com>",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "yarn clean && yarn build:pp && gh-pages -d public",
    "build:prod": "yarn clean && yarn build && yarn serve",
    "clean": "rm -rf .cache && rm -rf public",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:md",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public --ignore-pattern static .",
    "lint:md": "remark content/posts/",
    "format": "prettier '**/*.{js,jsx,css}' --write"
  },
  "dependencies": {
    "babel-eslint": "^10.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "classnames": "^2.2.5",
    "gatsby": "^1.9.279",
    "gatsby-link": "^1.6.46",
    "gatsby-pagination": "^1.1.0",
    "gatsby-plugin-catch-links": "^1.0.14",
    "gatsby-plugin-feed": "^1.3.15",
    "gatsby-plugin-google-analytics": "^1.0.14",
    "gatsby-plugin-manifest": "^1.0.10",
    "gatsby-plugin-nprogress": "^1.0.9",
    "gatsby-plugin-offline": "^1.0.12",
    "gatsby-plugin-react-helmet": "^1.0.2",
    "gatsby-plugin-sharp": "^2.3.13",
    "gatsby-plugin-sitemap": "^1.2.7",
    "gatsby-plugin-twitter": "^1.0.14",
    "gatsby-remark-autolink-headers": "^1.4.10",
    "gatsby-remark-copy-linked-files": "^1.5.23",
    "gatsby-remark-images": "^1.5.34",
    "gatsby-remark-prismjs": "^1.2.11",
    "gatsby-remark-responsive-iframe": "^1.4.16",
    "gatsby-source-filesystem": "^1.5.10",
    "gatsby-transformer-json": "^1.0.14",
    "gatsby-transformer-remark": "^1.7.24",
    "lodash": "^4.17.13",
    "lodash-webpack-plugin": "^0.11.4",
    "moment": "^2.20.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-gravatar": "^2.6.3",
    "react-helmet": "^5.1.3",
    "react-scroll": "^1.7.3",
    "react-share": "^1.15.1",
    "react-social-icons": "^2.7.0",
    "react-twitter-widgets": "^1.4.0"
  },
  "devDependencies": {
    "cli-glob": "0.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "gh-pages": "2.2.0",
    "plop": "2.6.0",
    "prettier": "1.19.1",
    "remark-cli": "4.0.0",
    "remark-preset-lint-recommended": "3.0.4"
  },
  "license": "MIT",
  "main": "n/a",
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
